<%@ taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c" %>
<%@ taglib uri="http://www.zkoss.org/dsp/web/theme" prefix="t" %>
body {
	background: ${t:gradValue('ver', '#007497 0%; #007497 38px; transparent 39px; transparent 100%')},
		url(${c:encodeURL('/images/icons/background.png')}) center 36px;
	<c:if test="${zk.ff > 0}">
	background: ${t:gradValue('ver', '#007497 0%; #007497 35px; transparent 35px; transparent 100%')},
		url(${c:encodeURL('/images/icons/background.png')}) center 36px;
	</c:if>
	<c:if test="${zk.ie <= 9}">
	background: url(${c:encodeURL('/images/icons/background-ie.png')}) center 36px;
	</c:if>
}
.z-tabs-header {
	height: 36px;
}
.z-tabs .z-tabs-cnt {
	border-bottom-color: transparent;
}
.z-tab .z-image {
	height: 24px;
	width: 32px;
	line-height: 28px;
}
.z-tabpanel {
	border-color: transparent;
}
.vbar {
	padding: 5px;
	border-right: 1px solid #CFCFCD;
}
.first-vbar {
	border-left: 1px solid #CFCFCD;
}
.status {
	border-top: 1px solid #CFCFCD;border-bottom: 1px solid #CFCFCD;
}
.number {
	font-weight: bold;
	color: black;
	font-family: Arial,sans-serif;
}
.text {
	font-family: Arial,sans-serif;
	font-size: 12px;	
}
.main-content,
.connects > div,
.stories > div {
	max-height: 750px;
}
.home {
	background: url(${c:encodeURL('/images/icons/icon_zk.png')}) no-repeat center center,
		${t:gradValue('ver', '#02ABDE 0%; #007497 100%')};
	<c:if test="${zk.ie <= 9}">
		background: url(${c:encodeURL('/images/icons/icon_zk.png')}) no-repeat center center;
	</c:if>
	${t:borderRadius('5px')};
	cursor: pointer;
}
.connect {
	background: url(${c:encodeURL('/images/icons/icon_friendrequests.png')}) no-repeat center center,
		${t:gradValue('ver', '#02ABDE 0%; #007497 100%')};
	<c:if test="${zk.ie <= 9}">
		background: url(${c:encodeURL('/images/icons/icon_friendrequests.png')}) no-repeat center center;
	</c:if>
	${t:borderRadius('5px')};
	cursor: pointer;
}
.discover {
	background: url(${c:encodeURL('/images/icons/icon_messagestop.png')}) no-repeat center center,
		${t:gradValue('ver', '#02ABDE 0%; #007497 100%')};
	<c:if test="${zk.ie <= 9}">
		background: url(${c:encodeURL('/images/icons/icon_messagestop.png')}) no-repeat center center;
	</c:if>
	${t:borderRadius('5px')};
	cursor: pointer;
}
.z-tab-seld .home {	
	background: url(${c:encodeURL('/images/icons/icon_zk_white.png')}) no-repeat center center,
		${t:gradValue('ver', '#02ABDE 0%; #007497 100%')};
		
	<c:if test="${zk.ie <= 9}">
	background: url(${c:encodeURL('/images/icons/icon_zk_blue.png')}) no-repeat center center;
	</c:if>
}
.z-tab-seld .connect {	
	background: url(${c:encodeURL('/images/icons/icon_friendrequests_white.png')}) no-repeat center center,
		${t:gradValue('ver', '#02ABDE 0%; #007497 100%')};
		
	<c:if test="${zk.ie <= 9}">
	background: url(${c:encodeURL('/images/icons/icon_friendrequests_blue.png')}) no-repeat center center;
	</c:if>
}
.z-tab-seld .discover {	
	background: url(${c:encodeURL('/images/icons/icon_messagestop_white.png')}) no-repeat center center,
		${t:gradValue('ver', '#02ABDE 0%; #007497 100%')};
	<c:if test="${zk.ie <= 9}">
	background: url(${c:encodeURL('/images/icons/icon_messagestop_blue.png')}) no-repeat center center;
	</c:if>
}
.author-icon {
	${t:borderRadius('50px')};
	${t:boxShadow('2px 2px 7px #02ABDE, -2px -2px 7px #02ABDE')};
	margin: 10px;
	width: 48px;
	height: 48px;
}
.connects .author-icon {
	${t:boxShadow('2px 2px 7px #84d89d, -2px -2px 7px #84d89d')};
}
.stories .author-icon {
	${t:borderRadius('2px')};
	background-size: 24px 24px;
	${t:boxShadow('none')};
	width: 24px;
	height: 24px;
	margin: 0 2px;
	vertical-align: middle;
}
.author {
	color: #555555;
	font-size: 14px;
	font-weight: bold;
}
.content {
	color: #333333;
}
.main {
	margin: auto;
}
.profile {
	margin: 0 10px 10px 0;
	${t:borderRadius('5px')};
	background: #F9F9F9;
}
.owner-icon {	
	${t:borderRadius('5px')};
	width: 48px;
	height: 48px;
}
.fullname {
	color: #0084B4;
	font-weight: bold;
	font-size: 14px;
}
.z-tabpanel {
	padding: 0;
}
<%-- avatars icons borrowed from http://www.Iconshock.com --%>
.alien {
	background-image: url('data:image/png;base64,');
}
.astronauta {
	background-image: url('data:image/png;base64,');
}
.bombero {
	background-image: url('data:image/png;base64,');	
}
.comisario {
	background-image: url('data:image/png;base64,');
}
.dreds {
	background-image: url('data:image/png;base64,');
}
.hiphopper {
	background-image: url('data:image/png;base64,');
}
.mimo {
	background-image: url('data:image/png;base64,');
}
.mounstruo {
	background-image: url('data:image/png;base64,');
}
@media only screen and (orientation:portrait) {
	body {
		margin: 0;
		padding: 0;
		${t:gradient('ver', '#90caee 0%;#e2f3fd 100%')};
	}
	.z-tabs-header {
		height: auto;
		background: black;
	}
	.z-tabs-cnt > li.z-tab,
	.z-tabs-cnt > .z-tab:active {
		background: transparent;
		${t:boxShadow('none')};
		border: 0;
		width: 128px;
		height: 47px;
	}
	.z-tab .z-label {
		display: none;
	}
	.z-tabs-cnt > li.z-tab.z-tab-seld,
	.z-tabs-cnt > li.z-tab.z-tab-seld:first-child,
	.z-tabs-cnt > li.z-tab.z-tab-seld:active,
	.z-tabs-cnt > li.z-tab.z-tab-seld:active:first-child {
		background: black;
		border-color: transparent;
		${t:boxShadow('1px 1px 0 black')};
	}
	.z-tabs-cnt > .z-tabs {
		background: #555;
	}
	td.z-caption-r {
		text-align: center;
	}
	.main-content,
	.connects > div,
	.stories > div {
		max-height: 2048px;
	}
	.home {
		background: url(${c:encodeURL('/images/icons/icon_zk_white.png')}) no-repeat center center;
	}
	.connect {
		background: url(${c:encodeURL('/images/icons/icon_friendrequests_white.png')}) no-repeat center center;
	}
	.discover {
		background: url(${c:encodeURL('/images/icons/icon_messagestop_white.png')}) no-repeat center center;
	}
	
	.z-tab .z-image {
		height: 47px;
		line-height: 28px;
		width: 80px;
	}
	tr.z-listbox-odd {
		${t:gradient('ver', '#e1e1e1 0%;#e1e1e1 100%')};
	}
	.z-scrollview-content-ver:first-child .profile {
		margin: 10px;
	}
	.profile {
		margin-left: 10px;
		border: 3px solid #CFCFCF;
		${t:borderRadius('15px')};
		${t:boxShadow('0 0 7px rgba(0, 0, 0, 0.70)')};
	}
	.z-groupbox-3d-cnt {
		border: 0px;
	}
}
 
@media screen and (orientation:portrait) and (max-width: 720px) {
	.main-content,
	.connects > div,
	.stories > div {
		max-height: 1024px;
	}
	.z-tabs-cnt > li.z-tab,
	.z-tabs-cnt > .z-tab:active {
		background: transparent;
		${t:boxShadow('none')};
		border: 0;
		width: 80px;
		height: 47px;
	}
	.z-tab .z-image {
		height: 47px;
		line-height: 28px;
		width: 60px;
	}
	.main-content > .z-hlayout-inner:first-child {
		display: none;
	}
}
